# Doxyfile for MyNetwork API

# Set the project name and version
PROJECT_NAME           = "turn-based game network API"
PROJECT_NUMBER         = "1.0"

# Set directories for input and output files
INPUT                  = network/include network/src
FILE_PATTERNS          = *.h *.cpp
RECURSIVE              = YES
OUTPUT_DIRECTORY       = docs

# Specify directories containing source code
INPUT_FILTER           = network/src/*.cpp

# Specify encoding
