# Specify if RTF output should generate colored text
RTF_HYPERLINKS         = YES

# Specify if the RTF output should include fonts
RTF_STYLESHEET_FILE    = ""

# Specify if the RTF output should include source code
RTF_SOURCE_CODE        = YES

# Specify if bookmarks should be included in the PDF
GENERATE_PDF           = YES

# Specify if the PDF should contain internal links
USE_PDFLATEX           = YES

# Specify if compact PDF files should be generated
COMPACT_PDF            = YES

# Specify if HTML links should be generated with relative paths
HTML_FILE_EXTENSION    = .html

# Specify if class documentation should be included in the HTML
RECURSIVE             = YES

# Specify the title of the HTML documentation
HTML_TITLE            = "MyNetwork API Documentation"

# Specify additional HTML stylesheets
HTML_EXTRA_STYLESHEET = 

# Specify additional HTML headers
HTML_EXTRA_FILES      = 

# Specify if you want to include the graph in the documentation
HAVE_DOT              = YES

# Specify the file format of the graph (PNG, SVG, etc.)
DOT_IMAGE_FORMAT      = png

# Specify if you want to include dot graphs in the documentation
CALL_GRAPH            = YES
CALLER_GRAPH          = YES
GRAPHICAL_HIERARCHY   = YES

# Specify if you want to include the source code in the documentation
SOURCE_BROWSER        = YES

# Specify if you want to enable search engine indexing
GENERATE_SEARCH       = YES

# Specify the ordered list of HTML header files
HTML_HEADER           = 
